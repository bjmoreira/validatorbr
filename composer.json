{
    "name": "bjmoreira/validatorbr",
    "description": "validação de campos utilizados no Brasil",
    "license": "MIT",
    "authors": [
        {
            "name": "Bruno Moreira",
            "email": "bbjomoreira@gmail.com",
            "homepage": "http://www.bmsoftware.com.br"
        }
    ],
    "homepage": "https://github.com/bjmoreira/validatorbr",
    "keywords": ["Laravel", "validatorbr"],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "illuminate/support": "~5|~6"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0",
        "mockery/mockery": "^1.1",
        "orchestra/testbench": "~3|~4",
        "sempro/phpunit-pretty-print": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "bjmoreira\\validatorbr\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "bjmoreira\\validatorbr\\Tests\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "bjmoreira\\validatorbr\\validatorbrServiceProvider"
            ],
            "aliases": {
                "validatorbr": "bjmoreira\\validatorbr\\Facades\\validatorbr"
            }
        }
    }
}
